{
    "name": "boilerplate-mvc-express-sequelize",
    "version": "1.0.0",
    "description": "",
    "main": "index.js",
    "scripts": {
        "start": "node -r dotenv/config bin/www",
        "dev": "nodemon -r dotenv/config bin/www",
        "db:create": "sequelize db:create",
        "db:drop": "sequelize db:drop",
        "db:migrate": "sequelize db:migrate",
        "db:unmigrate": "sequelize db:migrate:undo:all",
        "db:seed": "sequelize db:seed:undo:all && sequelize db:seed:all",
        "db:migrate:undo:all": "sequelize db:migrate:undo:all",
        "db:reset": "npm run db:drop && npm run db:create && npm run db:migrate && npm run db:seed",
        "test": "echo \"Error: no test specified\" && exit 1"
    },
